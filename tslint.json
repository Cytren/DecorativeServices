{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "arrow-parens": false,
        "space-before-function-paren": [true, "always"],
        "interface-name": [true, "never-prefix"],
        "no-console": [false],
        "ordered-imports": [true, {
            "import-sources-order": "any",
            "named-imports-order": "any"
        }],
        "ban-types": false
    },
    "rulesDirectory": []
}